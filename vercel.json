{
    "version": 2,
    "buildCommand": "bun run build",
    "installCommand": "bun install",
    "framework": null,
    "outputDirectory": "dist/client",
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/((?!assets/|favicon.ico|android-chrome-|apple-touch-icon|site.webmanifest).*|/)",
            "destination": "/api/index"
        }
    ]
}